{
  "compilerOptions":{
      "module":"commonjs",
      "esModuleInterop":true,
      "target":"es6",
      "noImplicitAny":true,
      "moduleResolution":"node",
      "sourceMap":true,
      "outDir":"../dist",
      "baseUrl":".",
      "plugins":[
          {
              "name":"typescript-tslint-plugin"
          }
      ],
      "types": [
        "node",
      ]
  },
  "include":[
      "./",
  ],
  "exclude":[
      "node_modules/*",
      "*_test.ts",
      "test_resources.ts"
  ]
}
